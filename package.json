    "cross-env": "^5.0.1",
    "eslint": "^4.4.1",
    "eslint-plugin-react": "^7.2.0",
    "expect": "^22.0.3",
    "jquery": "^3.1.1",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "laravel-mix": "^1.0",
    "lodash": "^4.17.4",
    "mocha": "^4.1.0",
    "mocha-webpack": "^1.0.1",
    "moxios": "^0.4.0",
    "mwangaben-vthelpers": "^1.1.3",
    "vue": "^2.1.10",
